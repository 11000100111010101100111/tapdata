{
  "properties": {
    "name": "DummyDB",
    "icon": "icons/dummy.png",
    "doc" : "${doc}",
    "id": "dummy db"
  },
  "configOptions": {
    "pdkExpansion": [],
    "connection": {
      "type": "object",
      "properties": {
        "mode": {"type": "string", "title": "${df_mode}", "x-decorator": "FormItem", "x-component": "Radio.Group", "x-index": 1, "required": true, "default": "Common", "x-component-props": {"optionType": "button"},
          "enum": [
            {"label": "${df_mode_options_common}", "value": "Common", "disabled": false},
            {"label": "${df_mode_options_heartbeat}", "value": "Heartbeat", "disabled": false}
          ],
          "x-reactions": [
            {"target": "*(initial_totals,table_name,table_fields)", "fulfill": {"state": {"display": "{{$self.value=='Common' ? 'visible' : 'hidden'}}"}}}
          ]
        },
        "initial_totals": {"type": "number", "title": "${df_initial_totals}", "x-decorator": "FormItem", "x-component": "InputNumber", "x-index": 2, "required": true, "default": "1", "x-reactions": {"dependencies": ["__TAPDATA.connection_type"], "fulfill": { "schema": { "x-decorator-props.style.display": "{{$deps[0]!=='target' ? null:'none'}}" } }}},
        "incremental_interval": {"type": "number", "title": "${df_incremental_interval}", "x-decorator": "FormItem", "x-component": "InputNumber", "x-index": 3, "required": true, "default": "0", "x-reactions": {"dependencies": ["__TAPDATA.connection_type"], "fulfill": { "schema": { "x-decorator-props.style.display": "{{$deps[0]!=='target' ? null:'none'}}" } }}},
        "incremental_interval_totals": {"type": "number", "title": "${df_incremental_interval_totals}", "x-decorator": "FormItem", "x-component": "InputNumber", "x-index": 4, "required": true, "default": "1", "x-reactions": {"dependencies": ["__TAPDATA.connection_type"], "fulfill": { "schema": { "x-decorator-props.style.display": "{{$deps[0]!=='target' ? null:'none'}}" } }}},
        "incremental_types": {"type": "array", "title": "${df_incremental_types}", "x-decorator": "FormItem", "x-component": "Checkbox.Group", "x-index": 5, "default": [1],
          "enum": [
            {"label": "${df_incremental_types_insert}", "value": 1},
            {"label": "${df_incremental_types_update}", "value": 2},
            {"label": "${df_incremental_types_delete}", "value": 3}
          ],
          "x-reactions": {"dependencies": ["__TAPDATA.connection_type"], "fulfill": { "schema": { "x-decorator-props.style.display": "{{$deps[0]!=='target' ? null:'none'}}" } } }
        },
        "write_interval": {"type": "number", "title": "${df_write_interval}", "x-decorator": "FormItem", "x-component": "InputNumber", "x-index": 6, "required": true, "default": "1000", "x-reactions": {"dependencies": ["__TAPDATA.connection_type"], "fulfill": { "schema": { "x-decorator-props.style.display": "{{$deps[0]!=='source' ? null:'none'}}" } }}},
        "write_interval_totals": {"type": "number", "title": "${df_write_interval_totals}", "x-decorator": "FormItem", "x-component": "InputNumber", "x-index": 7, "required": true, "default": "1", "x-reactions": {"dependencies": ["__TAPDATA.connection_type"], "fulfill": { "schema": { "x-decorator-props.style.display": "{{$deps[0]!=='source' ? null:'none'}}" } }}},
        "write_log": {"type": "boolean", "title": "${df_write_log}", "x-decorator": "FormItem", "x-component": "Switch", "x-index": 8, "required": true, "default": "false", "x-reactions": {"dependencies": ["__TAPDATA.connection_type"], "fulfill": { "schema": { "x-decorator-props.style.display": "{{$deps[0]!=='source' ? null:'none'}}" } }}},
        "table_name": {"type": "string", "title": "${df_table_name}", "x-decorator": "FormItem", "x-component": "Input", "x-index": 9, "required": true, "default": "tapd8_dummy"},
        "table_fields": {"type": "array", "title": "${df_table_fields}", "x-decorator": "FormItem", "x-component": "ArrayTable", "x-index": 10, "items": {
            "type": "object",
            "properties": {
              "c1": {"type": "void", "x-component": "ArrayTable.Column", "x-index": 1, "x-component-props": {"title": "${df_table_field_name}"}, "properties": {"name": {"type": "string", "x-decorator": "FormItem", "x-component": "Input"}}},
              "c2": {"type": "void", "x-component": "ArrayTable.Column", "x-index": 2, "x-component-props": {"title": "${df_table_field_type}"}, "properties": {"type": {"type": "string", "x-decorator": "FormItem", "x-component": "Select", "default": "String",
                "enum": [
                  {"label": "String", "value": "String"},
                  {"label": "Number", "value": "Number"},
                  {"label": "Boolean", "value": "Boolean"},
                  {"label": "Date", "value": "Date"},
                  {"label": "Array", "value": "Array"},
                  {"label": "Binary", "value": "Binary"},
                  {"label": "Map", "value": "Map"},
                  {"label": "Time", "value": "Time"},
                  {"label": "DateTime", "value": "DateTime"}
                ]
              }}},
              "c3": {"type": "void", "x-component": "ArrayTable.Column", "x-index": 3, "x-component-props": {"width": 80, "title": "${df_table_field_length}" }, "properties": {"len": {"type": "string", "x-decorator": "Editable", "x-component": "Input"}}},
              "c4": {"type": "void", "x-component": "ArrayTable.Column", "x-index": 4, "x-component-props": {"width": 100, "title": "${df_table_field_default}" }, "properties": {"def": {"type": "string", "x-decorator": "FormItem", "x-component": "Select", "x-component-props": {"allowCreate": true, "filterable": true},
                "enum": [
                  {"label": "", "value": ""},
                  {"label": "serial(1,1)", "value": "serial(1,1)"},
                  {"label": "now()", "value": "now()"},
                  {"label": "randomLong(9999)", "value": "randomLong(9999)"},
                  {"label": "randomString(8)", "value": "randomString(8)"},
                  {"label": "uuid()", "value": "uuid()"}
                ]}}},
              "c5": {"type": "void", "x-component": "ArrayTable.Column", "x-index": 5, "x-component-props": {"width": 80, "title": "${df_table_field_primary}" }, "properties": {"pri": {"type": "boolean", "x-decorator": "FormItem", "x-component": "Switch", "default": "false"}}},
              "c6": {"type": "void", "x-component": "ArrayTable.Column", "x-index": 6, "x-component-props": {"width": 100, "title": "${df_table_field_operator}", "prop": "operations", "fixed": "right"}, "properties": {"operator": {"type": "void", "x-component": "FormItem",
                "properties": {
                  "remove": {"type": "void", "x-component": "ArrayTable.Remove"},
                  "moveDown": {"type": "void", "x-component": "ArrayTable.MoveDown"},
                  "moveUp": {"type": "void", "x-component": "ArrayTable.MoveUp"}
                }
              }}}
            }
          },
          "properties": {"add": {"type": "void", "x-component": "ArrayTable.Addition", "title": "${df_table_field_btn_add}"}}
        }
      }
    }
  },
  "messages": {
