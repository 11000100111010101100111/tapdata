  "dataTypes": {
    "String[($byte)][fixed]": {"byte" : "16m", "fixed" : "fixed", "to" : "TapString"},
    "Number[($precision, $scale)]": {"precision" : [1, 40], "precisionDefault" : 4, "scale" : [0, 10], "scaleDefault" : 1, "to": "TapNumber"},
    "Boolean" : {"to": "TapBoolean"},
    "Date" : {"to": "TapDate"},
    "Array" : {"to": "TapArray"},
    "Binary" : {"to": "TapBinary"},
    "Map" : {"to": "TapMap"},
