{
  "properties": {
    "name": "Lark-Doc",
    "icon": "icons/lark-doc.png",
    "doc" : "${doc}",
    "id": "lark-doc",
    "authentication": "Beta",
    "tags": ["SaaS"]
  },
  "configOptions": {
    "pdkExpansion": [],
    "node": {
      "type": "object",
      "properties": {
      }
    },
    "connection": {
      "type": "object",
      "properties": {
        "teamName": {
          "type": "String",
          "title": "${teamName}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_host",
          "x-index": 10,
          "required": true
        },
        "token": {
          "type": "String",
          "title": "${token}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_host",
          "x-index": 20,
          "required": true,
          "x-display": "hidden"
        },
        "code": {
          "type": "String",
          "title": "${oauth_code}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-decorator-props": {
            "style": {"display": "none"}
          },
          "apiServerKey": "code",
          "x-index": 30,
          "required": true
        },
        "projectName": {
          "title": "${projectName}",
          "type": "string",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "AsyncSelect",
          "x-component-props": {
            "method": "{{loadCommandList}}",
            "params": "{{ {$values: $values, command: \"DescribeUserProjects\"} }}"
          },
          "x-index": 40,
          "x-reactions": [
            {
              "dependencies": ["teamName", "token"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0]&&$deps[1] ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        },
        "line": {
          "type": "void",
          "x-component": "Space",
          "x-reactions": [
            {
              "dependencies": ["teamName"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0] ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ],
          "x-decorator": "FormItem",
          "title": " ",
          "x-decorator-props": {
            "colon": false
          },
          "properties": {
            "authButton": {
              "type": "void",
              "x-decorator": "FormItem",
              "x-component": "Button",
              "x-content": "${oauth_title}",
              "title": "",
              "x-index": 40,
              "x-component-props": {
                "type": "primary",
                "onClick": "{{useAsyncDataSourceByConfig({service: goToAuthorized, withoutField: true}, { target: 'https://redirect.tapdata.io/oauth/complete', pdkId: 'coding', oauthUrl: 'https://open.feishu.cn/open-apis/authen/v1/index?redirect_uri=https://redirect.tapdata.io/oauth/complete/lark&app_id=cli_a375a91e7879900b&state=lark_doc_state'})}}"
              },
              "x-decorator-props":{"style":{"width":"120px"}}
            },
            "OAuthStatus": {
              "type": "void",
              "title": "",
              "x-decorator": "FormItem",
              "x-component": "Text",
              "apiServerKey": "AAA",
              "x-index": 50,
              "required": true,
              "x-component-props": {
                "content": "{{$values.code ? '${oauth_ok}' : '${oauth_msg}'}}",
                "style": {"color": "{{ $values.code ? 'green' : 'rgb(83, 95, 114)' }}"}
              }
            }
          },
          "x-index": 50
        },
        "clientId": {
          "type": "String",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "default": "f62b86d98192df7d72f286fa086d3d85",
          "x-display": "hidden",
          "index": 998
        },
        "clientSecret":{
          "type": "String",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "default": "b76e9308d43810ed775d953d8b3a33c1ad2fd5db",
          "x-display": "hidden",
          "index": 999
        }
      }
    }
  },
  "messages": {
    "default": "en_US",
    "en_US": {
      "doc": "docs/lark-doc_en_US.md",
      "oauth_code": "Authorize",
      "oauth_title": "Authorize",
      "oauth_ok": "Authorization successfully",
      "oauth_msg": "Please authorize before connection test and save"
    },
    "zh_CN": {

      "doc": "docs/lark-doc_zh_CN.md",
      "oauth_code": "授权",
      "oauth_title": "授权",
      "oauth_ok": "授权成功",
      "oauth_msg": "请先授权成功再点击连接测试或保存"
    },
    "zh_TW": {
      "doc": "docs/lark-doc_zh_TW.md",

      "oauth_code": "授權",
      "oauth_title": "授權",
      "oauth_ok": "授權成功",
      "oauth_msg": "請先授權成功再點擊鏈接測試或保存"
    }
  }
}
